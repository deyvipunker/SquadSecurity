@page "/habilitadores/edit/{Id:int}"
<h3>Editar Habilitador</h3>
@if (habilitador is null)
{
    <Loading />
}
else
{
    <NavigationLock OnBeforeInternalNavigation="OnBeforeInternalNavigation" />

    <EditForm EditContext="editContex" OnValidSubmit="EditAsync">
       
        <button class="btn btn-success" @onclick="Return">Regresar</button>
    </EditForm>
}

